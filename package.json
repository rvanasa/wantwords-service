{
  "name": "wantwords-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node .",
    "start-dev": "concurrently -r npm:watch-*",
    "watch-client": "cd client && npm run start",
    "watch-server": "nodemon .",
    "deploy": "git add . && cd client && npm run build && git add -f build && git commit -m \"Deploy\" && git push heroku master"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "block-parser": "1.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "express": "^4.17.1",
    "glob-promise": "^3.4.0",
    "simple-git": "^1.131.0",
    "glob": "^7.1.6",
    "dotenv": "latest",
    "esm": "^3.2.25"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "concurrently": "^5.1.0"
  },
  "engines": {
    "node": "12.x"
  }
}
